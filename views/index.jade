      script(src='https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js')
      script(src='https://oss.maxcdn.com/libs/respond.js/1.4.2/respond.min.js')
    body#page-top.index
        // Navigation
        nav.navbar.navbar-default.navbar-fixed-top
            .container
                // Brand and toggle get grouped for better mobile display
                .navbar-header.page-scroll
                    button.navbar-toggle(type='button', data-toggle='collapse', data-target='#bs-example-navbar-collapse-1')
                        span.sr-only Toggle navigation
                        span.icon-bar
                        span.icon-bar
                        span.icon-bar
                    img.ronc-logo(src='img/ronclogo.png', alt='')
                    a.navbar-brand(href="#page-top") ФБГНУ»РОНЦ им.Н.Н.Блохина»
                // Collect the nav links, forms, and other content for toggling
                #bs-example-navbar-collapse-1.collapse.navbar-collapse
                    ul.nav.navbar-nav.navbar-right
                        li.hidden
                            a(href='#page-top')
                        li.page-scroll
                            a(href='#about') Об отделении
                        li.page-scroll
                            a(href='#portfolio') Хобби
                        li.page-scroll
                            a(href='#contact') Контакты
                // /.navbar-collapse
            // /.container-fluid
        // Header
        header
            .container
                .row
                    .col-lg-12
                        //img.img-responsive.photo(src='img/photo.jpg', alt='')
                        img.img-responsive.photo(src='img/hayrapet_sedrakyan_square.jpg', style="width:300px")
                        .intro-text
                            span.name Седракян Айрапет Ншанович
                            span.skills К.м.н., Заведующий отделением ортопедической стоматологии
        // About Section
        section#about
            .container
                .row
                    .col-lg-12.text-center
                        h2 Об отделении
                        hr
                .row
                    .col-lg-8.col-lg-offset-2
                        h4 
                            | С 199Х года Отделением руководит, кандидат медицинских наук, автор многочисленных патентов Седракян А.Н..
                        p
                            | Отделение занимается изготовлением сложных челюстно-лицевых протезов,аппаратов,обтураторов,эктопротезов лица, а так же полных съемных, частично съемных и современных несъёмных протезов.
        // Portfolio Grid Section
        section#portfolio.success
            .container
                .row
                    .col-lg-12.text-center
                        h2 Хобби
                        hr(style="border-color: #fff;")
                .row
                    .col-sm-4.portfolio-item
                        a.portfolio-link(href='#portfolioModal1', data-toggle='modal')
                            .caption
                                .caption-content
                                    i.fa.fa-search-plus.fa-2x
                            img.img-responsive(src='img/portfolio/sculpture00_rotated.jpg', alt='')
                    .col-sm-4.portfolio-item
                        a.portfolio-link(href='#portfolioModal2', data-toggle='modal')
                            .caption
                                .caption-content
                                    i.fa.fa-search-plus.fa-2x
                            img.img-responsive(src='img/portfolio/sculpture01_rotated.jpg', alt='')
        // Contact Section
        section#contact
            .container
                .row
                    .col-lg-12.text-center
                        h2 Контакты
                        hr
                .row
                    .col-lg-8.col-lg-offset-2
                        div
                            span(style="font-weight: bold;")
                                | Наш адрес: 
                            span 
                                | 115478, г. Москва, Каширское шоссе, д.23
                        div
                            span(style="font-weight: bold;")
                                | Телефон: 
                            span
                                | 499 324-55-12 
                        div
                            span(style="font-weight: bold;")
                                | Факс: 
                            span
                                | 499 324-55-12
                        div
                            span(style="font-weight: bold;")
                                | Часы работы: 
                            span
                                | с понедельника по четверг 8:30-15:00 пятница-8:30-14:00
                        //div
                            span(style="font-weight: bold;")
                                | Как добраться-Карта
        // Footer
        footer.text-center
            iframe(width="100%", height="300", frameborder="0", style="border:0", src="https://www.google.com/maps/embed/v1/place?key=AIzaSyBhosed48fee5J9V-w9rWTq0EIpPdhYntM&q=Kashirskoye+sh.,+23,+Moskva,+Russia,+115478")
            .footer-below
                .container
                    .row
                        .col-lg-12
                            | Copyright &copy; Hayrapet Sedrakyan 2015
        // Scroll to Top Button (Only visible on small and extra-small screen sizes)
        .scroll-top.page-scroll.visible-xs.visble-sm
